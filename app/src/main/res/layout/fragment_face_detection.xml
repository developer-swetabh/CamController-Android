    android:layout_height="match_parent"
    tools:context="com.swetabh.camcontroller.ui.robocam.FaceDetectionFragment">

    <com.swetabh.camcontroller.customui.CameraSourcePreview
        android:id="@+id/preview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <com.swetabh.camcontroller.customui.GraphicOverlay
        android:id="@+id/faceOverlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</FrameLayout>
